                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
Proveedor.Folio AS FOLIO,
Proveedor.A_Paterno +' ' + Proveedor.A_Materno +' '+Proveedor.Nombre AS NOMBRE, 
Proveedor.Calle AS CALLE, 
Proveedor.Num_Ext AS NoEXT, 
Proveedor.Num_int AS NoINT, 
Proveedor.Telefono AS TELEFONO, 
Proveedor.RFC, 
Proveedor.Email AS CORREO, 
Proveedor.ID_Colonia as "ID Colonia",
Municipio.IDMunicipio AS "ID MUNICIPIO",
Municipio.Nombre AS "NOMBRE MUNICIPIO",
Localidad.IDLocalidad AS "ID LOCALIDAD",
Localidad.Nombre AS "NOMBRE LOCALIDAD",
Colonia.IDColonia AS "ID COLONIA" ,
Colonia.Nombre AS "NOMBRE COLONIA",
Proveedor.A_Paterno,
Proveedor.A_Materno,
Proveedor.Nombre 
FROM Proveedor
INNER JOIN Colonia ON ID_Colonia=IDColonia
INNER JOIN Localidad ON ID_Localidad=IDLocalidad
INNER JOIN Municipio ON ID_Municipio=IDMunicipio
where Proveedor.NOMBRE like '%' +@Nombre+'%'  </CommandText>
